{
  "name": "react-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:development": "cross-env CUSTOM_ENV=development node scripts/start.js",
    "start:test": "cross-env CUSTOM_ENV=test node scripts/start.js",
    "start:production": "cross-env CUSTOM_ENV=production node scripts/start.js",
    "build:development": "yarn eslint && cross-env CUSTOM_ENV=development node scripts/build.js",
    "build:test": "yarn eslint && cross-env CUSTOM_ENV=test node scripts/build.js",
    "build:production": "yarn eslint && cross-env CUSTOM_ENV=production node scripts/build.js",
    "build": "yarn clear:webpackCache && yarn clear:buildDist && yarn build:production",
    "build:development:preview": "yarn build:development && yarn preview",
    "build:test:preview": "yarn build:test && yarn preview",
    "build:production:preview": "yarn build:production && yarn preview",
    "clear:webpackCache": "rimraf ./node_modules/.cache",
    "clear:buildDist": "rimraf ./build",
    "test": "jest --config ./jest.config.ts",
    "eslint": "cross-env NODE_ENV=production eslint --fix ./src",
    "prepare": "husky install",
    "preview": "serve -s -u ./build",
    "git:commit": "yarn eslint && git add . && git commit -m \"feat: git:commit\" && git push origin master",
    "init": "rm -rf ./node_modules && rm -rf build && rm -rf coverage && yarn"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "yarn eslint"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/core": "7.12.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "@svgr/webpack": "^6.2.1",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.177",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "@uiw/react-md-editor": "^3.12.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "8.1.0",
    "babel-plugin-named-asset-import": "^0.3.7",
    "babel-preset-react-app": "^10.0.0",
    "bfj": "^7.0.2",
    "browserslist": "^4.18.1",
    "camelcase": "^6.2.1",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "eslint": "^7.11.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.9.2",
    "eslint-webpack-plugin": "^2.5.2",
    "file-loader": "6.1.1",
    "fs-extra": "^9.0.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "^27.5.1",
    "jest-circus": "28.0.3",
    "jest-resolve": "26.6.0",
    "jest-watch-typeahead": "1.1.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.4.5",
    "modern-normalize": "^1.1.0",
    "postcss": "^8.4.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.0.1",
    "progress-webpack-plugin": "^1.0.16",
    "prompts": "2.4.0",
    "rc-virtual-list": "^3.4.7",
    "react": "^17.0.2",
    "react-app-polyfill": "^2.0.0",
    "react-dev-utils": "^12.0.0",
    "react-dom": "^17.0.2",
    "react-refresh": "^0.11.0",
    "react-router-dom": "5.2.1",
