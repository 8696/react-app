    "webpack-manifest-plugin": "^4.0.2",
    "workbox-webpack-plugin": "^6.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/qs": "^6.9.7",
    "ahooks": "^3.3.10",
    "antd": "^4.20.2",
    "axios": "^0.26.1",
    "babel-plugin-import": "^1.13.3",
    "cross-env": "^7.0.3",
    "less": "^4.1.2",
    "less-loader": "^5.0.0",
    "lint-staged": "^12.1.2",
    "qs": "^6.10.1",
    "serve": "^13.0.2"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd",
          "style": "css"
        }
      ]
    ]
  }
}
