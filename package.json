{
  "name": "react-app",
  "version": "1.0.0",
  "private": false,
  "license": "MIT",
  "scripts": {
    "start:development": "cross-env CUSTOM_ENV=development node scripts/start.js",
    "build:development": "yarn eslint && cross-env CUSTOM_ENV=development node scripts/build.js",
    "start:test": "cross-env CUSTOM_ENV=test node scripts/start.js",
    "build:test": "yarn eslint && cross-env CUSTOM_ENV=test node scripts/build.js",
    "start:production": "cross-env CUSTOM_ENV=production node scripts/start.js",
    "build:production": "yarn eslint && cross-env CUSTOM_ENV=production node scripts/build.js",
    "analyze": "yarn clear:cache && yarn eslint && cross-env CUSTOM_ENV=production ANALYZE=true node scripts/build.js",
    "eslint": "cross-env NODE_ENV=production eslint --fix ./src",
    "prepare": "husky install",
    "preview": "serve -s -u ./build",
    "git:commit": "yarn eslint && git add . && git commit -m \"feat: git:commit\" && git push origin master",
    "clear:package": "rm -rf ./node_modules && rm -rf build && rm -rf coverage",
    "init:package": "yarn clear:package && yarn install",
    "clear:cache": "rm -rf ./node_modules/.cache",
    "init:cusenv": "node ./customEnv.js",
    "ncu:check": "ncu --target minor -f 'antd,@ant-design/icons,@ant-design/cssinjs,ahooks'",
    "ncu:update": "ncu --target minor -u -f 'antd,@ant-design/icons,@ant-design/cssinjs,ahooks' && yarn install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "yarn eslint"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@ant-design/cssinjs": "^2.0.1",
    "@babel/core": "7.28.3",
    "@commitlint/cli": "17.8.1",
    "@commitlint/config-conventional": "17.8.1",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.17",
    "@svgr/webpack": "6.5.1",
    "@types/node": "16.18.126",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-router": "5.1.20",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "autoprefixer": "10.4.21",
    "babel-loader": "8.4.1",
    "babel-plugin-named-asset-import": "0.3.8",
    "babel-preset-react-app": "10.1.0",
    "bfj": "7.1.0",
    "browserslist": "4.25.2",
    "camelcase": "6.3.0",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "cross-env": "7.0.3",
    "css-loader": "6.11.0",
    "css-minimizer-webpack-plugin": "3.4.1",
    "dayjs": "^1.11.13",
    "dotenv": "8.6.0",
    "dotenv-expand": "5.1.0",
    "eslint": "8.57.1",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-webpack-plugin": "2.7.0",
    "file-loader": "6.2.0",
    "fs-extra": "10.1.0",
    "html-webpack-plugin": "5.6.4",
    "http-proxy-middleware": "2.0.9",
    "husky": "8.0.3",
    "less": "4.3.0",
    "less-loader": "10.2.0",
    "lint-staged": "12.5.0",
    "mini-css-extract-plugin": "2.9.4",
    "moment": "^2.30.1",
    "postcss": "8.5.6",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-loader": "6.2.1",
    "postcss-normalize": "10.0.1",
    "postcss-preset-env": "7.8.3",
    "progress-webpack-plugin": "1.0.16",
    "rc-picker": "^4.11.3",
    "react": "^19.2.0",
    "react-dev-utils": "12.0.1",
    "react-dom": "^19.2.0",
    "react-refresh": "0.17.0",
    "react-router-dom": "^6",
    "recorder-core": "^1.3.25011100",
    "resolve": "1.22.10",
    "resolve-url-loader": "3.1.5",
    "sass": "1.89.0",
    "sass-loader": "10.5.2",
    "semver": "7.7.2",
    "serve": "14.2.4",
    "source-map-loader": "3.0.2",
    "style-loader": "3.3.4",
    "terser-webpack-plugin": "5.3.14",
    "ts-node": "10.9.2",
    "ts-pnp": "1.2.0",
    "typescript": "5.1.6",
    "url-loader": "4.1.1",
    "web-vitals": "1.1.2",
    "webpack": "5.101.2",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-dev-server": "4.15.2",
    "webpack-manifest-plugin": "4.1.1",
    "workbox-webpack-plugin": "6.6.1"
  },
  "devDependencies": {
    "@ant-design/icons": "^6.1.0",
    "@types/lodash": "4.17.20",
    "@types/qs": "6.14.0",
    "ahooks": "3.9.6",
    "antd": "^6.0.0",
    "await-to-js": "3.0.0",
    "axios": "0.30.1",
    "axios-mock-adapter": "1.22.0",
    "lodash": "4.17.21",
    "qs": "6.14.0",
    "tailwindcss": "3.4.17"
  },
  "resolutions": {
    "@types/react": "18.3.23",
    "@types/react-dom": "18.3.7"
  },
  "engines": {
    "node": ">=16.0.0",
    "yarn": ">=1.22.0"
  }
}