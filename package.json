    "tailwindcss": "^3.0.2",
    "terser-webpack-plugin": "^5.2.5",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "ts-pnp": "1.2.0",
    "typescript": "4.3.4",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "4.1.1",
    "web-vitals": "^1.0.1",
    "webpack": "^5.72.0",
