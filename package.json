{
  "name": "react-app",
  "version": "1.0.0",
  "private": false,
  "license": "MIT",
  "scripts": {
    "start:development": "cross-env CUSTOM_ENV=development node scripts/start.js",
    "build:development": "yarn eslint && cross-env CUSTOM_ENV=development node scripts/build.js",
    "start:test": "cross-env CUSTOM_ENV=test node scripts/start.js",
    "build:test": "yarn eslint && cross-env CUSTOM_ENV=test node scripts/build.js",
    "start:production": "cross-env CUSTOM_ENV=production node scripts/start.js",
    "build:production": "yarn eslint && cross-env CUSTOM_ENV=production node scripts/build.js",
    "analyze": "yarn clear:cache && yarn eslint && cross-env CUSTOM_ENV=production ANALYZE=true node scripts/build.js",
    "test": "jest --config ./jest.config.ts",
    "eslint": "cross-env NODE_ENV=production eslint --fix ./src",
    "prepare": "husky install",
    "preview": "serve -s -u ./build",
    "git:commit": "yarn eslint && git add . && git commit -m \"feat: git:commit\" && git push origin master",
    "clear:package": "rm -rf ./node_modules && rm -rf build && rm -rf coverage",
    "init:package": "yarn clear:package && yarn",
    "update:package": "rm -rf ./yarn.lock && yarn init:package",
    "clear:cache": "rm -rf ./node_modules/.cache",
    "init:cusenv": "node ./customEnv.js"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "yarn eslint"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.18.5",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "@svgr/webpack": "^6.2.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.11.41",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.25.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.5",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-named-asset-import": "^0.3.7",
    "babel-preset-react-app": "^10.0.0",
    "bfj": "^7.0.2",
    "browserslist": "^4.18.1",
    "camelcase": "^6.3.0",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "eslint": "^8.17.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-webpack-plugin": "^2.5.2",
    "file-loader": "6.2.0",
    "fs-extra": "^10.1.0",
    "html-webpack-plugin": "^5.5.0",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^12.1.2",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss": "^8.4.14",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.0.1",
    "progress-webpack-plugin": "^1.0.16",
    "react": "^17.0.2",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^17.0.2",
    "react-refresh": "^0.13.0",
    "react-router-dom": "^5.3.3",
    "resolve": "1.22.0",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.43.5",
    "sass-loader": "^10.0.5",
    "semver": "7.3.2",
    "serve": "^14.0.1",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.8.1",
    "ts-pnp": "1.2.0",
    "typescript": "4.7.4",
    "url-loader": "4.1.1",
    "web-vitals": "^1.0.1",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-dev-server": "^4.9.2",
    "webpack-manifest-plugin": "^4.1.1",
    "workbox-webpack-plugin": "^6.4.1"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@types/lodash": "^4.14.182",
    "@types/qs": "^6.9.7",
    "ahooks": "^3.5.2",
    "antd": "^4.21.6",
    "await-to-js": "^3.0.0",
    "axios": "^0.27.2",
    "axios-mock-adapter": "^1.21.1",
    "lodash": "^4.17.21",
    "qs": "^6.11.0"
  }
}
