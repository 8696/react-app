{
  "name": "react-app",
  "version": "1.0.0",
  "private": false,
  "license": "MIT",
  "scripts": {
    "start:development": "cross-env CUSTOM_ENV=development node scripts/start.js",
    "build:development": "yarn eslint && cross-env CUSTOM_ENV=development node scripts/build.js",
    "start:test": "cross-env CUSTOM_ENV=test node scripts/start.js",
    "build:test": "yarn eslint && cross-env CUSTOM_ENV=test node scripts/build.js",
    "start:production": "cross-env CUSTOM_ENV=production node scripts/start.js",
    "build:production": "yarn eslint && cross-env CUSTOM_ENV=production node scripts/build.js",
    "analyze": "yarn clear:cache && yarn eslint && cross-env CUSTOM_ENV=production ANALYZE=true node scripts/build.js",
    "test": "jest --config ./jest.config.ts",
    "eslint": "cross-env NODE_ENV=production eslint --fix ./src",
    "prepare": "husky install",
    "preview": "serve -s -u ./build",
    "git:commit": "yarn eslint && git add . && git commit -m \"feat: git:commit\" && git push origin master",
    "clear:package": "rm -rf ./node_modules && rm -rf build && rm -rf coverage",
    "init:package": "yarn clear:package && yarn",
    "update:package": "rm -rf ./yarn.lock && yarn init:package",
    "clear:cache": "rm -rf ./node_modules/.cache",
    "init:cusenv": "node ./customEnv.js"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "yarn eslint"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.20.2",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.8",
    "@svgr/webpack": "^6.5.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.18.3",
    "@types/react": "^17.0.52",
    "@types/react-dom": "^17.0.18",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.3.0",
    "babel-plugin-import": "^1.13.5",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-preset-react-app": "^10.0.1",
    "bfj": "^7.0.2",
    "browserslist": "^4.21.4",
    "camelcase": "^6.3.0",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "dotenv": "8.6.0",
    "dotenv-expand": "5.1.0",
    "eslint": "^8.27.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-webpack-plugin": "^2.7.0",
    "file-loader": "6.2.0",
    "fs-extra": "^10.1.0",
    "html-webpack-plugin": "^5.5.0",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "less": "^4.1.3",
    "less-loader": "^10.2.0",
    "lint-staged": "^12.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.18",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.8.2",
    "progress-webpack-plugin": "^1.0.16",
    "react": "^17.0.2",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^17.0.2",
    "react-refresh": "^0.14.0",
    "react-router-dom": "^5.3.4",
    "resolve": "1.22.1",
    "resolve-url-loader": "^3.1.4",
    "sass": "^1.56.0",
    "sass-loader": "^10.3.1",
    "semver": "7.3.8",
    "serve": "^14.0.1",
    "source-map-loader": "^3.0.2",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.9.1",
    "ts-pnp": "1.2.0",
    "typescript": "4.8.4",
    "url-loader": "4.1.1",
    "web-vitals": "^1.1.2",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-manifest-plugin": "^4.1.1",
    "workbox-webpack-plugin": "^6.5.4"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@types/lodash": "^4.14.188",
    "@types/qs": "^6.9.7",
    "ahooks": "^3.7.2",
    "antd": "^4.24.1",
    "await-to-js": "^3.0.0",
    "axios": "^0.27.2",
    "axios-mock-adapter": "^1.21.2",
    "lodash": "^4.17.21",
    "qs": "^6.11.0",
    "tailwindcss": "^3.2.2"
  }
}
